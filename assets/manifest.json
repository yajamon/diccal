{
    "manifest_version": 2,
    "name": "diccal",
    "version": "0.1.0",
    "description": "開いているウィンドウを1px単位で調整する拡張機能です！",

    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "browser_action": {
        "default_icon": {
            "19": "icons/icon19.png"
        },
        "default_title": "diccal",
        "default_popup": "html/popup.html"
    },

    "permissions": ["tabs", "<all_urls>"]
}
